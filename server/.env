
# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration 
DB_HOST=localhost
DB_PORT=3306
DB_NAME=tecace_devices
DB_USER=root
DB_PASSWORD=password

# Session Configuration
SESSION_SECRET=tecace-device-secret-key

# Client URL
CLIENT_URL=http://localhost:8080

# Force Development Mode (set to true to bypass database connection)
FORCE_DEV_MODE=true

# Force the frontend to use offline mode (no actual server connection attempts)
SKIP_DB_CONNECTION=true
